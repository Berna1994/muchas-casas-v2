{
  "hosting": {
    "public": "dist/muchas-casas2/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/login.html",
        "destination": "/login.html"
      },
      {
        "source": "/singup.html",
        "destination": "/singup.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
